import QtQuick
import QtQuick.Controls 2.15
import QtQuick.Layouts 1.15

ApplicationWindow {
    id: main
    width: 150
    height: 150
    maximumHeight: height
    maximumWidth: width
    minimumHeight: height
    minimumWidth: width
    visible: true
    title: qsTr("Time Tracker")
    flags: Qt.FramelessWindowHint | Qt.WindowStaysOnTopHint
    color: "transparent"

    QtObject{
        id: images
        readonly property string move: 'data:image/png;base64,'
        readonly property string minimize: "data:image/png;base64,"
    }

    Component.onCompleted: {
        console.log(Screen.width)
        console.log(Screen.height)
        setX(Screen.width - (width+20))
        setY(Screen.height - height*1.5)
    }

    Rectangle{
        width: 20
        height: 20
        radius: width/2
        color: white
        visible: active

        Image{
            width: parent.width
            height: parent.height
            source: images.move
        }
    }

    ColumnLayout{
        Layout.fillHeight: true
        Layout.fillWidth: true

        Rectangle{
            id: rect
            width: 150
            height: 150
            radius: width/2
            color: "red"

            Behavior on opacity{
                NumberAnimation {
                    target: rect
                    property: "opacity"
                    duration: 1000
                    easing.type: Easing.InOutQuad
                }
            }


            Text{
                anchors.centerIn: parent
                text: "Start"

                MouseArea{
                    anchors.fill: parent
                    onClicked: {
                        console.log('I am clicked')
                    }
                }
            }
        }
    }

    onActiveChanged: {
        if(active){
            showNormal()
            rect.opacity = 1;
        }else{
            trigger.repeat = true
            rect.opacity = 0.4
        }

    }

    Timer{
        id: trigger
        running: !active
        interval: 120000
        repeat: true
        onTriggered: {
            if(rect.opacity === 04) rect.opacity = 0.2;
            else {
                showMinimized();
                console.log('Here now, window should be minimized');
                repeat = false
            }
        }
    }

}
